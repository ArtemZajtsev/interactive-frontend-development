{
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "mocha":true
  },
  "plugins": ["import"],
  "extends": [
    "eslint:recommended",
    "google",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "globals": {
    "expect":true
  },
  rules: {
    "require-jsdoc": "off",
    "max-len": ["error", 120],
    "comma-dangle": "off",
    "linebreak-style": "off"
  }
}